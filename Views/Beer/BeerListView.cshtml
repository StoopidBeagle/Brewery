@model IEnumerable<app.Models.Beer>

<table>
    <tr>
        <th>Brewer</th>
        <th>Name</th>
        <th>Type</th>
    </tr>
    @foreach (var beer in Model)
    {
    <tr>
        <td>@beer.Brewer</td>
        <td><a href="beers/@beer.ID">@beer.Name</a></td>
        <td>@beer.Type</td>
    </tr>
    }
</table>
